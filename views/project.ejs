    <script>

      function toggleVisibility(activeId) {
        const sections = ["ALL", "AI", "Design", "Business", "MobileDevelopment", "CustomSoftwareDevelopment", "Fintech", "Consulting", "News", "Outstaffing", "TechTrends", "IoT", "Construction","Blockchain","Ecommerce","Hospitality"];

        sections.forEach(id => {
          const element = document.getElementById(id);
          element.style.display = (id === activeId) ? "flex" : "none";
        });

        sessionStorage.setItem('activeSection', activeId);
      }

      function ALL() {
        toggleVisibility("ALL");
      }

      function AI() {
        toggleVisibility("AI");
      }

      function Design() {
        toggleVisibility("Design");
      }

      function Business() {
        toggleVisibility("Business");
      }

      function MobileDevelopment() {
        toggleVisibility("MobileDevelopment");
      }

      function CustomSoftwareDevelopment() {
        toggleVisibility("CustomSoftwareDevelopment");
      }

      function Fintech() {
        toggleVisibility("Fintech");
      }

      function Consulting() {
        toggleVisibility("Consulting");
      }

      function News() {
        toggleVisibility("News");
      }

      function Outstaffing() {
        toggleVisibility("Outstaffing");
      }

      function TechTrends() {
        toggleVisibility("TechTrends");
      }

      function IoT() {
        toggleVisibility("IoT");
      }

      function Construction() {
        toggleVisibility("Construction");
      }

      function Blockchain() {
        toggleVisibility("Blockchain");
      }

      function Ecommerce() {
        toggleVisibility("Ecommerce");
      }

      function Hospitality() {
        toggleVisibility("Hospitality");
      }









      window.onload = function () {
        const activeSection = sessionStorage.getItem('activeSection') || 'ALL'; // Default to "ALL" if not set
        toggleVisibility(activeSection);
      };

    </script>


